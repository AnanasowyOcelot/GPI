{% block block %}
    <h3 class="sonata-feed-title">{{ settings.title }}</h3>

    <div class="sonata-feeds-container">
        {% for offer in offers %}
            <div class="block-newsletter col-sm-12 well">
                <h5 class="panel-title truncate">Name: {{ offer.name }}</h5>
                <a href="{{ path('gpi_offer_details', {'id': offer.id }) }}">Details</a>
            </div>
        {% endfor %}
    </div>
{% endblock %}
